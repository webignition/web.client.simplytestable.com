{
    "name": "simplytestable/webclient",
    "description": "web client to interface with app.simplytestable.com",
    "autoload": {
        "psr-4": { "": "src/" },
        "files": ["app/AppKernel.php", "app/AppCache.php"]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" },
        "files": [ "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php" ]
    },
    "require": {
        "php": ">=5.6.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "incenteev/composer-parameter-handler": "^2.0",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^3.0.2",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/symfony": "3.3.*",
        "twig/twig": "^1.0||^2.0",
        "symfony/assetic-bundle": "~2.8",
        "doctrine/migrations": "~1.2",
        "doctrine/doctrine-migrations-bundle": "~1.2",
        "webignition/url":">=1.9.10,<2.0",
        "guzzlehttp/guzzle": "~5.2",
        "guzzlehttp/retry-subscriber":">=2.0.2,<3",
        "webignition/guzzle-curl-exception":">=0.1.4,<1",
        "webignition/html-validation-error-normaliser":"dev-master",
        "webignition/html-validation-error-linkifier":"dev-master",
        "stephenfarrell/postmark-bundle": "dev-master#09eb492d79e823ee256f75b2d2f16df01dc5d146",
        "egulias/email-validator":"1.1.1",
        "doctrine/collections":"~1.2",
        "webignition/php-stripe-models":">=0.2.6,<1.0",
        "jeremykendall/php-domain-parser": "1.2.1",
        "willdurand/negotiation": "1.3.3",
        "chrisboulton/php-resque": "dev-master#e393d5672cdbe1e94e856a6cb2b2ec97042a4f24",
        "chrisboulton/php-resque-scheduler": "dev-master#ab8bd52949cac1f815bbb97aa39f51c7b5766a86",
        "resquebundle/resque": "1.2.0",
        "webignition/resque-job-factory": ">=0.1.1,<1.0",
        "webignition/php-ie-detector": "0.1",
        "webignition/simplytestable-user-model": ">=0.1,<1",
        "webignition/simplytestable-user-serializer": ">=1.1,<2"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0",
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "1.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "php bin/console assetic:dump"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ],
        "test-unit": "./vendor/bin/phpunit --colors tests/WebClientBundle/Unit/",
        "test-functional": "./vendor/bin/phpunit --colors tests/WebClientBundle/Functional/",
        "test": [
            "@composer test-unit",
            "@composer test-functional"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-var-dir": "var",
        "symfony-bin-dir": "bin",
        "symfony-tests-dir": "tests",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "3.3-dev"
        }
    }
}
