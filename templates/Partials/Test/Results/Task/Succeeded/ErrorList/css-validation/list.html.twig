<ul class="refs grouped-issues collapse in" id="error-list-content">
    {% for ref,errors in errors_by_ref %}
        {% set display_ref = ref is empty ? task.url : ref %}

        <li class="error-group">
            <h3>
                <a href="{{ display_ref|escape('html') }}">
                    <i class="fa fa-file-code-o"></i>{{ display_ref }}
                    <span class="badge alert-danger issue-count">{{ errors|length }}</span>
                </a>
            </h3>

            <ul class="issues css-validation-issues">
                {% for message in errors %}
                    <li class="issue error">
                        {% include 'Partials/Test/Results/Task/Succeeded/ErrorList/css-validation/error.html.twig' %}
                    </li>
                {% endfor %}
            </ul>
        </li>
    {% endfor %}
</ul>