{% set queue_label_types = {
    queued: 'warning',
    in_progress: 'primary',
    completed: 'success',
    skipped: 'default',
    failed: 'danger'
} %}

{% for queue_name, queue_label_type in queue_label_types %}
    {% include 'Partials/Test/Progress/Index/task-queue.html.twig'
        with {
        'name': queue_name|capitalize,
        'value': remote_test.taskcountbystate[queue_name],
        'label_type': queue_label_type
    } %}
{% endfor %}
